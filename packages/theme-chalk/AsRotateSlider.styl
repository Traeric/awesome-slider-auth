.as-rotate-slider-wrap
    width 100%
    .as-rotate-slider-bg
        position relative
        .as-rotate-slider-canvas
            width 100%
            margin-bottom 5px
            border-radius 5px
        .as-rotate-slider-rotate
            position absolute
            border-radius 255px
            border 2px solid #323232
            box-shadow 0 0 12px #78e183 inset

