.as-top
    position fixed
    cursor pointer
    transition bottom .5s
    .default
        height 40px
        width 40px
        border-radius 6px
        background-color #fff
        display flex
        justify-content center
        align-items center
        box-shadow 0 1px 8px 0 rgba(0, 0, 0, 0.1)
        .icon-top
            width 22px
            height 22px
